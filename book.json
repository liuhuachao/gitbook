{
    "title": "GitBook 简明教程",
    "author": "Liu Huachao <liuhuachao@foxmail.com>",
    "description": "A GitBook about learning GitBook",
    "language": "zh-hans",
    "extension": null,
    "generator": "site",
    "links": {
        "sidebar": {
            "About Me": "http://www.liuhuachao.com"
        }
    },
    "output": null,
    "pdf": {
        "fontSize": 12,
        "footerTemplate": null,
        "headerTemplate": null,
        "margin": {
            "bottom": 36,
            "left": 62,
            "right": 62,
            "top": 36
        },
        "pageNumbers": false,
        "paperSize": "a4"
    },
    "plugins": [
        "theme-comscore",
        "github",
        "splitter",
        "chapter-fold",
        "back-to-top-button",
        "-sharing",
        "sharing-plus",
        "-search",
        "search-pro",
        "fontsettings"
    ],
    "pluginsConfig": {
        "theme-default": {
            "showLevel": true
        },
        "github": {
            "url": "https://github.com/liuhuachao/gitbook"
        },
        "sharing": {
            "twitter": true,
            "weibo": true,
            "all": [
                "twitter", "facebook", "google", "weibo", "qzone", "douban"
            ]
        }
    },
    "variables": {}
}